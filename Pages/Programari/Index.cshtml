    <tbody>
@foreach (var item in Model.Programare) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DataProgramare)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Pacient.ID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Procedura.ID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Medic.ID)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.ID">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.ID">Details</a> |
